{
    "name": "senky/akismet",
    "type": "phpbb-extension",
    "description": "Akismet Anti-Spam Extension for phpBB",
    "version": "1.2.1",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Matt Friedman",
            "homepage": "http://mattfriedman.me",
            "role": "Past Developer"
        },
        {
            "name": "Jakub Senko",
            "email": "jakubsenko@gmail.com",
            "role": "Extension Developer"
        },
        {
            "name": "Matt Gibson",
            "email": "matt@mattgibsoncreative.co.uk",
            "homepage": "http://mattgibsoncreative.co.uk",
            "role": "Original extension author and developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gothick/php-akismet"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "~1.0.0",
        "gothick/php-akismet": "dev-master",
        "phpbb/phpbb": ">=3.2.6"
    },
    "require-dev": {
        "phing/phing": "2.4.*",
        "senky/phpbb-coding-standard": "dev-master"
    },
    "extra": {
        "display-name": "Akismet Anti-Spam Extension",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.6"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/akismet",
            "filename": "version_check",
            "ssl": true
        }
    }
}